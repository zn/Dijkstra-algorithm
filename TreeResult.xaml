<Window x:Class="Graph.TreeResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graph"
        mc:Ignorable="d"
        Title="Результат" Height="450" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Canvas Name="resultCanvas"></Canvas>
        <TextBlock Grid.Row="2" Name="resultPrice" Text="Размер минимальной затраты " Foreground="Green" Margin="5" HorizontalAlignment="Center" FontSize="20"/>
    </Grid>
</Window>
